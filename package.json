{
    "name": "vite-project",
    "version": "2.20.3",
    "private": true,
    "scripts": {
        "dev": "vite",
        "node:dev": "cross-env NODE_ENV=dev nodemon --exec babel-node ./server/main.js",
        "build": "node ./server/version.js && run-p type-check build-only && pnpm run node:babel && pnpm run node:babel:watch && node ./server/zip.js",
        "node:babel": "babel server/main.js --out-file dist/server.js",
        "node:babel:watch": "babel server/watch.js --out-file dist/watch.js",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/ && prettier --write server/"
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.3.1",
        "axios": "^1.4.0",
        "compressing": "^1.9.0",
        "crypto-js": "^4.2.0",
        "element-plus": "^2.3.6",
        "koa": "^2.14.2",
        "koa-body": "^6.0.1",
        "koa-bodyparser": "^4.4.0",
        "koa2-cors": "^2.0.6",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "pako": "^2.1.0",
        "pinia": "^2.0.36",
        "qs": "^6.11.2",
        "sass": "^1.63.3",
        "vue": "^3.3.2",
        "vue-router": "^4.2.0",
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.25.9",
        "@babel/core": "^7.22.8",
        "@babel/node": "^7.22.6",
        "@babel/preset-env": "^7.22.7",
        "@rushstack/eslint-patch": "^1.2.0",
        "@tsconfig/node18": "^2.0.1",
        "@types/axios": "^0.14.0",
        "@types/lodash": "^4.14.195",
        "@types/node": "^18.16.8",
        "@types/qs": "^6.9.7",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vitejs/plugin-vue-jsx": "^3.0.1",
        "@vue/eslint-config-prettier": "^7.1.0",
        "@vue/eslint-config-typescript": "^11.0.3",
        "@vue/tsconfig": "^0.4.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.22.0",
        "eslint-plugin-vue": "^9.11.0",
        "https": "^1.0.0",
        "koa-router": "^12.0.0",
        "koa-static": "^5.0.0",
        "nodemon": "^2.0.22",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.8",
        "typescript": "~5.0.4",
        "vite": "^4.3.5",
        "vue-tsc": "^1.6.4"
    }
}